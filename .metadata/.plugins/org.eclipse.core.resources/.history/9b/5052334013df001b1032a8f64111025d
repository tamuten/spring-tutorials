package jp.tuyano.spring.sample1;

import java.util.Calendar;
import java.util.Date;

public class MyBean {
	private Date date;
	private String message;
	public MyBean() {
		super();
		this.date = Calendar.getInstance().getTime();
	}
	
	public MyBean(String message) {
		this();
		this.message = message;
	}
	
	
}
